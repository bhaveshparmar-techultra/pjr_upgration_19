<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">

        <record id="hr_salary_rule_EMP_INSURANCE_DED_category" model="hr.salary.rule">
            <field name="amount_select">code</field>
            <field name="code">EMP_INSURANCE_DED</field>
            <field name="category_id" ref="hr_payroll.DED"/>
            <field name="name">Employee Insurance</field>
            <field name="sequence" eval="99"/>
            <field name="condition_select">python</field>
            <field name="condition_python">result = version.active_employee_insurance and version.started_deduction_date.month &lt;= payslip.date_from.month and version.remaining_emp_period > 0</field>
            <field name="amount_select">code</field>
            <field name="amount_python_compute">result = -version.employee_ins_amount / version.emp_ins_period</field>
            <field name="struct_id" ref="hr_payroll.structure_002"/>
        </record>

